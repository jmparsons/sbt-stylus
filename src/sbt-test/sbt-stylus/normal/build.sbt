checkMainCssContents := {
  val contents = IO.read((WebKeys.public in Assets).value / "css" / "main.css")
  val expectedContents = """body {
                           |  color: #fff;
                           |  display: flexbox;
                           |  -webkit-box-shadow: 2px 2px 2px #000;
                           |  box-shadow: 2px 2px 2px #000;
                           |}
                           |.inline-1 {
                           |  background: url("../images/nature-8kb.jpg");
                           |}
                           |.inline-2 {
                           |  background: url("../images/nature-23kb.jpg");
                           |}
                           |.inline-3 {
                           |  background: url("../images/nature-36kb.jpg");
                           |}
                           |@import 'ext.css';
                           |""".stripMargin
  if (contents != expectedContents) sys.error(s"checkMainCssContents expected: $expectedContents \nrecieved: $contents")
}

